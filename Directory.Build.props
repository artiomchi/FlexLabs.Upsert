<Project>

  <PropertyGroup>
    <ImplicitUsings>enable</ImplicitUsings>
    <ReproducibleBuildsVersion>1.2.39</ReproducibleBuildsVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DotNet.ReproducibleBuilds" Version="$(ReproducibleBuildsVersion)" PrivateAssets="All"/>
  </ItemGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>..\..\certs\FlexLabsStrongKey.snk</AssemblyOriginatorKeyFile>
    <PublicKey></PublicKey>
  </PropertyGroup>

</Project>